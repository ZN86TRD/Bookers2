<div class="container px-5 px-sm-0">
  <div class="row">

    <div class="col-md-4">
      
      <h2>User info</h2>
      <%= render 'userinfo', user: @user %>

      <hr>
      
      <h2 class="mt-3">New book</h2>
      <%= render 'books/newbook', book: @book %>
      
    </div>
    <div class="col-md-8">
      
      <h2>Books</h2>
      <%= render 'books/books', books: @books %>
      
    </div>
  </div>
</div>
