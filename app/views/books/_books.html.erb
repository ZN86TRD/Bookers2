<!--投稿一覧-->
<% @books.each do |book| %>

    <div>

        <p><%= image_tag book.user.get_profile_image(100,100) %></p>

        <!--詳細画面リンク-->
        <p>Title---<%= link_to book_path(book.id) do %>
          <%= book.title %></p>
        <% end %>
        <p>Opinion---<%= book.body %></p>
    </div>

<% end %>